package course.oop.other;

public final class RowColDia {
	
	public static final OnePair[]  rowA = {new OnePair(0,0), new OnePair(0,1), new OnePair(0,2)};
	public static final OnePair[]  rowB = {new OnePair(1,0), new OnePair(1,1), new OnePair(1,2)};
	public static final OnePair[]  rowC = {new OnePair(2,0), new OnePair(2,1), new OnePair(2,2)};
	
	public static final OnePair[]  colA = {new OnePair(2,0), new OnePair(2,1), new OnePair(2,2)};
	public static final OnePair[]  colB = {new OnePair(2,0), new OnePair(2,1), new OnePair(2,2)};
	public static final OnePair[]  colC = {new OnePair(2,0), new OnePair(2,1), new OnePair(2,2)};
	
	public static final OnePair[] diagA = {new OnePair(2,0), new OnePair(2,1), new OnePair(2,2)};
	public static final OnePair[] diagB = {new OnePair(2,0), new OnePair(2,1), new OnePair(2,2)};
}