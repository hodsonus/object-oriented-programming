package course.oop.other;

import java.util.Arrays;
import java.util.Collections;
import java.util.List;

public final class RowColDia {
	
	public static final List<OnePair>  rowA = Collections.unmodifiableList(Arrays.asList(new OnePair(0,0), new OnePair(0,1), new OnePair(0,2)));
	public static final OnePair[]  rowB = {new OnePair(1,0), new OnePair(1,1), new OnePair(1,2)};
	public static final OnePair[]  rowC = {new OnePair(2,0), new OnePair(2,1), new OnePair(2,2)};
	
	public static final OnePair[]  colA = {new OnePair(0,0), new OnePair(1,0), new OnePair(2,0)};
	public static final OnePair[]  colB = {new OnePair(0,1), new OnePair(1,1), new OnePair(2,1)};
	public static final OnePair[]  colC = {new OnePair(0,2), new OnePair(1,2), new OnePair(2,2)};
	
	public static final OnePair[] diagA = {new OnePair(0,0), new OnePair(1,1), new OnePair(2,2)};
	public static final OnePair[] diagB = {new OnePair(0,2), new OnePair(1,1), new OnePair(2,0)};
}