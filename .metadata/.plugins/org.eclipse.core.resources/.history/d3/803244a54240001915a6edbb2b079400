package course.oop.other;

import java.util.Arrays;
import java.util.Collections;
import java.util.List;

public final class RowColDia {
	
	public static final List<OnePair> rowA  = Collections.unmodifiableList(Arrays.asList(new OnePair(0,0), new OnePair(0,1), new OnePair(0,2)));
	public static final List<OnePair> rowB  = Collections.unmodifiableList(Arrays.asList(new OnePair(1,0), new OnePair(1,1), new OnePair(1,2)));
	public static final List<OnePair> rowC  = Collections.unmodifiableList(Arrays.asList(new OnePair(2,0), new OnePair(2,1), new OnePair(2,2)));
	
	public static final List<OnePair> colA  = Collections.unmodifiableList(Arrays.asList(new OnePair(0,0), new OnePair(1,0), new OnePair(2,0)));
	public static final List<OnePair> colB  = Collections.unmodifiableList(Arrays.asList(new OnePair(0,1), new OnePair(1,1), new OnePair(2,1)));
	public static final List<OnePair> colC  = Collections.unmodifiableList(Arrays.asList(new OnePair(0,2), new OnePair(1,2), new OnePair(2,2)));
	
	public static final List<OnePair> diagA = Collections.unmodifiableList(Arrays.asList(new OnePair(0,0), new OnePair(1,1), new OnePair(2,2)));
	public static final List<OnePair> diagB = Collections.unmodifiableList(Arrays.asList(new OnePair(0,2), new OnePair(1,1), new OnePair(2,0)));
}