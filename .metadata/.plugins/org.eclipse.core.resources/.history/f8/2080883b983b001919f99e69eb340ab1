package course.oop.other;

public class OnePair implements Pair {

    public OnePair(int row, int col) {
    	this.row = row;
    	this.col = col;
    }
    
    @Override
    public boolean equals(Object b) {
    	if (!(b instanceof OnePair)) return false;
    	OnePair otherPair = (OnePair)(b);
    	return this.row==otherPair.row && this.col==otherPair.col;
    }
    
    @Override
    public int hashCode() {
        return (int)(Math.pow(2,15));
    }
    
    public int col;
    public int row;
}